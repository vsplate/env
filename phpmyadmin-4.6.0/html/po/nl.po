#: libraries/advisory_rules.txt:341
#, php-format
msgid "Immediate table locks %%"
msgstr "Onmiddellijke tabelvergrendelingen %%"

#: libraries/advisory_rules.txt:344 libraries/advisory_rules.txt:351
